{
    "data": {
        "input_path": "demo_data.parquet",
        "train_test_split_date": "2024-07-06",
        "train_val_split_ratio": 0.8,
        "datetime_col": "datetime",
        "instrument_col": "instrument",
        "feature_start_idx": 2,
        "feature_end_idx": 133,
        "label_col": "label_vwap_5m",
        "normalize": true,
        "handle_missing": "forward_fill"
    },
    "model": {
        "name": "FNN",
        "hidden_dims": [
            128,
            64,
            32
        ],
        "dropout_rate": 0.3,
        "activation": "leaky_relu",
        "batch_norm": true
    },
    "training": {
        "batch_size": 256,
        "num_epochs": 20,
        "learning_rate": 0.0001,
        "weight_decay": 1e-06,
        "early_stopping_patience": 8,
        "gradient_clip_value": 5.0,
        "optimizer": "adam",
        "scheduler": "cosine",
        "scheduler_params": {
            "T_max": 20
        }
    },
    "experiment": {
        "name": "futures_demo",
        "seed": 42,
        "save_dir": "./demo_experiments",
        "log_interval": 10,
        "save_best_only": true
    },
    "feature_engineering": {
        "add_time_features": true,
        "add_rolling_features": false,
        "rolling_windows": [
            5,
            10
        ]
    }
}