{
    "data": {
        "input_path": "../final_filtered_data_1min.parquet",
        "train_test_split_date": "2024-12-14",
        "train_val_split_ratio": 0.889,
        "datetime_col": "datetime",
        "instrument_col": "instrument",
        "feature_start_idx": 2,
        "feature_end_idx": 133,
        "label_col": "label_vwap_5m",
        "normalize": true,
        "handle_missing": "forward_fill"
    },
    "model": {
        "name": "FNN",
        "hidden_dims": [
            256,
            128,
            64
        ],
        "dropout_rate": 0.3,
        "activation": "leaky_relu",
        "batch_norm": true
    },
    "training": {
        "batch_size": 512,
        "num_epochs": 100,
        "learning_rate": 0.001,
        "weight_decay": 0.0001,
        "early_stopping_patience": 15,
        "gradient_clip_value": 5.0,
        "optimizer": "adamw",
        "scheduler": "cosine",
        "scheduler_params": {
            "T_max": 100,
            "eta_min": 1e-06
        }
    },
    "experiment": {
        "name": "futures_fnn_cross_sectional",
        "seed": 42,
        "save_dir": "./experiments",
        "log_interval": 100,
        "save_best_only": true
    },
    "feature_engineering": {
        "add_time_features": true,
        "cross_sectional_normalize": true,
        "add_rolling_features": false,
        "rolling_windows": [
            5,
            10,
            20
        ]
    }
}